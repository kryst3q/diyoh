{% extends "::base.html.twig" %}

{% block title %}DiyohBundle:Project:showTools{% endblock %}

{% block body %}

    <p>{{ tool.id }}</p>
    <p>{{ tool.name }}</p>
    
    {% for tag in tool.tags %}
        
        <p>{{ tag.name }}</p>
        
    {% endfor %}
    
    {% for project in tool.projects %}
        
        <a href="{{ url("diyoh_project_showproject",{'id':project.id}) }}"><p>{{ project.name }}</p></a>
        
    {% endfor %}
    
{% endblock %}
